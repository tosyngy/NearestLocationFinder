                                <?php
                                $more_nav = array();
                                foreach ($page_nav as $key => $value) {
                                    $met="";
                                    foreach ($page_nav_sub as $key1 => $value2) {
                                        if ($value2["nav_id"] == $value["name"]) {
                                            $met="set";
                                            if (!isset($more_nav[$value['name']])) {
                                                $i[$value['name']] = 0;
                                                $more_nav[$value['name']] = "";
                                            }
                                            if ($i[$value['name']] % 5 == 0) {
                                                $more_nav[$value['name']].= "  <div class='col-lg-3 col-md-6 col-sm-6 col-megamenu' style='height: 336px;'> <div class='megamenu-block'>  <ul>";
                                            }
                                            $more_nav[$value['name']].= " <li><a href='" . URL . $value2['url'] . "'><i class='fa fa-arrow-circle-right'></i>" . $value2['name'] . "</a></li>";
                                            if ($i[$value['name']]++ % 5 == 4) {
                                                $more_nav[$value['name']].="</ul></div>  </div>";
                                            }

                                            continue;
                                        }
                                    }
                                    if(!empty($met)){
                                        continue;
                                    }
                                    ?>           
                                    <li class="">
                                        <a href="<?php echo URL . $value["url"]; ?>"> <?php echo $value["name"]; ?> </a>
                                    </li>
                                <?php }
                                ?>
                                <?php
                                foreach ($more_nav as $key => $value) {
                                    ?>
                                    <li class="dropdown yamm-fw">
